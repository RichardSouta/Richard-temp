<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8">
    <meta name="description" content="collectors' nest je aplikace pro všechny sběratele."/>
    <meta name="keywords" content="collectors' nest, collectorsnest.eu, collecting, sběratelství, collectible, club">
    <meta name="author" content="collectors' nest"/>
    <meta name="copyright" content="collectors' nest"/>
    <meta name="application-name" content="collectors' nest"/>
    <!-- for Facebook -->
    <meta property="og:title" content="collectors' nest"/>
    <meta property="og:type" content="article"/>
    <meta property="og:url" content="http://www.collectorsnest.eu/"/>
    <meta property="og:description" content="collectors' nest je aplikace pro všechny sběratele."/>
    <!-- for Twitter -->
    <meta name="twitter:card" content="summary"/>
    <meta name="twitter:title" content="collectors' nest"/>
    <meta name="twitter:description" content="collectors' nest je aplikace pro všechny sběratele."/>
    <link rel="shortcut icon" href="{$basePath}/favicon.ico">
    <link rel="icon" href="{$basePath}/favicon.ico">
    <title>{ifset title}{include title|striptags} | {/ifset} collectors' nest</title>

    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css"
          integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">

    <!-- Optional theme -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap-theme.min.css"
          integrity="sha384-fLW2N01lMqjakBkx3l/M9EahuwpSfeNvV63J5ezn3uZzapT0u7EYsXMjQV+0En5r" crossorigin="anonymous">
    <!-- Latest jQuerry-->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="{$basePath}/js/nette.ajax.js"></script>
    <script src="{$basePath}/js/main.js"></script>
    <script src="{$basePath}/js/netteForms.js"></script>


    <!-- Latest compiled and minified JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"
            integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS"
            crossorigin="anonymous"></script>
    <link rel="stylesheet" type="text/css" href="{$basePath}/css/style.css">
</head>
<body>
<!-- Hlavička -->
<nav class="navbar navbar-default navbar-fixed-top" id="header">
    <div class="container-fluid">
        <div class="navbar-header" id="logo">
            <a href="{link Homepage:}">collectors' nest</a>
        </div>
        <div id="ccc"><a n:href="Club:">Sběratelský&nbsp;klub</a></div>
        <nav class="navbar navbar-default navbar-fixed-top" id="header2">
            <div class="container-fluid">
                {control searchForm}
                <div class="navbar-right">
                    <form id="paypal" action="https://www.paypal.com/cgi-bin/webscr" method="post" target="_top">
                        <input type="hidden" name="cmd" value="_s-xclick">
                        <input type="hidden" name="encrypted" value="-----BEGIN PKCS7----------END PKCS7-----
">
                        <input type="image" src="https://www.paypalobjects.com/en_US/i/btn/btn_donate_LG.gif" border="0"
                               name="submit" alt="PayPal - The safer, easier way to pay online!">
                        <img alt="" border="0" src="https://www.paypalobjects.com/en_US/i/scr/pixel.gif" width="1"
                             height="1">
                    </form>
                </div>
            </div>
        </nav>
        <div class="navbar-right" n:snippet="user_controls">
            {ifset $hide} {include 'hideModal.latte'} {/ifset}
            {if (!($user->isLoggedIn()))}
                <button type="button" class="btn btn-success" data-toggle="modal" data-target="#myModal">Přihlásit
                </button>
                <a href="{link Register:}" class="btn btn-primary">Registrovat</a>
            {else}
                <a href="{link Chat:}"><span id="messageIcon" class="glyphicon glyphicon-envelope" aria-hidden="true"></span></a>
            <a href="{link Notification:}"><span id="notificationIcon" class="glyphicon glyphicon-list-alt" aria-hidden="true"></span></a>
                <div class="dropdown">
                    <button class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu1"
                            data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
                        {$user->getIdentity()->username}
                        <span class="caret"></span>
                    </button>
                    <ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
                        <li><a href="{link User:,$user->identity->id}">Profil</a></li>
                        <li><a href="{link Collectible:new}">Přidat předmět</a></li>
                        {*<li><a href="{link Chat:}">Moje nastavení</a></li>*}
                        <li role="separator" class="divider"></li>
                        <li><a href="{link Logout!}" class="ajax">Odhlásit</a></li>
                    </ul>
                </div>
            {/if}
        </div>
    </div>
</nav>


<!--tělo-->
<div class="container-fluid" id="homepage">

    <div id="intro">

        {block intro}{/block}

    </div>
    <div class="col-xs-12" id="content">
        <div n:snippet="flashMessages">
            <div n:foreach="$flashes as $flash" class="alert alert-{$flash->type}">
                <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span
                            aria-hidden="true">&times;</span></button>

                {$flash->message}

            </div>
        </div>
        {block content}{/block}
    </div>

    <!--patička
      <div class="col-xs-12" id="footer">
        <div class="container-fluid">
          <div class="col-xs-4">
            <p>collectors' nest</p>
          </div>
          <div class="col-xs-4">
            <p>2016</p>
          </div>
          <div class="col-xs-4">
            <p>Have you read the <a href="{link Terms:}">Terms &amp; Conditions</a> yet?</p>
          </div>
        </div>
      </div>-->
</div>


<!-- Modal -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                            aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel">Přihlášení</h4>
            </div>
            <div class="modal-body">
                <div n:snippet="sign">
                    {control signInForm}
                </div>
                <a href="{link LostPassword:}" id="forgot_pass">
                    <button class="btn btn-info btn-xs">Zapomenuté heslo?</button>
                </a>
            </div>
        </div>
    </div>
</div>
{include 'loadMore.latte'}
{include 'Images.latte'}
<script>$('.required label').append('<required>*</required>');</script>
<link href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/css/select2.min.css" rel="stylesheet"/>
<script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/js/select2.min.js"></script>
<script type="text/javascript">
    $('select').select2();
</script>

</body>
</html>